        System.out.println(x+"\t"+cube);
    }
    System.out.println();
    System.out.println("Y+\t+Decreasing Integer");
    for(y=0;y<=98;y+=2){
        decInt=decInt-y;
        System.out.println(y+"\t"+decInt);
    }
    String a = "The quick brown fox jumped over the lazy dog";
